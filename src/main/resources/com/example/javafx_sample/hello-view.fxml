<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="helloViewRoot" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0"
            prefWidth="600.0" stylesheets="@helloview/style.css" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx_sample.helloview.HelloController">
    <children>
        <HBox fx:id="appbar" prefHeight="40.0" prefWidth="200.0" styleClass="appbar" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox fx:id="searchBackground" prefHeight="40.0" prefWidth="200.0" HBox.hgrow="ALWAYS" alignment="CENTER">
                    <children>
                        <TextField fx:id="searchField" onAction="#handleTextField" alignment="CENTER" layoutX="38.0"
                                   layoutY="7.0" maxHeight="25.0"
                                   prefHeight="25.0" prefWidth="350.0" promptText="Search" styleClass="search-bar"
                                   stylesheets="@helloview/style.css"/>
                    </children>
                </HBox>
                <ToolBar prefHeight="40.0" prefWidth="200.0" stylesheets="@helloview/style.css" HBox.hgrow="ALWAYS">
                    <items>
                        <Button mnemonicParsing="false" text="Button"/>
                    </items>
                </ToolBar>
            </children>
        </HBox>
    </children>
</AnchorPane>
